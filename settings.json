{
    "remote.SSH.remotePlatform": {
        "10.0.0.2": "linux"
    },
    "security.allowedUNCHosts": [
        "10.0.0.2"
    ],
    "workbench.startupEditor": "none",
    "window.zoomLevel": 1,
    "files.autoSave": "afterDelay",
    "vim.mouseSelectionGoesIntoVisualMode": false,
    "vim.autoSwitchInputMethod.enable": true,
